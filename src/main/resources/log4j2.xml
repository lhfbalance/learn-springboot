<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss}] %-5p %c{1} (%M\:%L) - %m%n"/>
        </Console>

    </Appenders>
    <Loggers>
        <root level="INFO">
            <AppenderRef ref="Console"/>
        </root>
    </Loggers>
</Configuration>
